<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tianling.blog.dao.MessageInfoMapper">
    <cache-ref namespace="com.tianling.blog.dao.MessageInfoMapper"/>
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tianling.blog.beans.MessageInfo">
        <id column="message_id" property="messageId" />
        <result column="message_content" property="messageContent" />
        <result column="message_time" property="messageTime" />
        <result column="message_name" property="messageName" />
        <result column="message_mark" property="messageMark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        message_id, message_content, message_time, message_name, message_mark
    </sql>

</mapper>
